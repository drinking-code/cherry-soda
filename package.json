{
  "name": "cherry-soda",
  "description": "Just another (super cool) JavaScript web framework.",
  "version": "0.1.2-experimental",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drinking-code/cherry-soda.git"
  },
  "bugs": {
    "url": "https://github.com/drinking-code/cherry-soda/issues"
  },
  "main": "src/index.ts",
  "module": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./bun": "./src/server/bun.ts",
    "./jsx-runtime": "./src/jsx-runtime.ts",
    "./jsx-dev-runtime": "./src/jsx-dev-runtime.ts"
  },
  "imports": {
    "#cherry-soda": "./src/index.ts",
    "#messages/colors": {
      "bun": "./src/messages/colors.bun.ts",
      "default": "./src/messages/colors.browser.ts"
    }
  },
  "scripts": {
    "test": "cd test && bun run test",
    "test:node": "cd test && npm run test:jest",
    "example": "cli/index dev example/cherry-soda-template/index.jsx",
    "example-counter": "cli/index dev example/counter/index.jsx"
  },
  "bin": {
    "cherry-soda": "cli/index"
  },
  "type": "module",
  "framework": {
    "displayName": "cherry-soda",
    "css": "onimportcss"
  },
  "devDependencies": {
    "@babel/types": "^7.19.3",
    "@types/babel__core": "^7.20.0",
    "@types/jest": "^29.2.0",
    "bun-types": "^0.1.11",
    "lodash": "^4.17.21",
    "typescript": "^4.9.5",
    "ws": "^8.9.0"
  },
  "dependencies": {
    "@babel/core": "^7.21.0",
    "@babel/generator": "^7.21.3",
    "@babel/parser": "^7.19.3",
    "@babel/traverse": "^7.21.2",
    "babel-plugin-minify-dead-code-elimination": "^0.5.2",
    "babel-plugin-remove-unused-import": "^2.1.1",
    "browserslist": "^4.21.4",
    "chalk": "^5.1.2",
    "commander": "^10.0.1",
    "css-modules-extractor": "^0.1.0-experimental",
    "esbuild": "^0.17.12",
    "html-tags": "^3.2.0",
    "hybridfs": "drinking-code/hybridfs",
    "mime": "^3.0.0",
    "open": "^8.4.2",
    "ora": "^6.3.0",
    "postcss": "^8.4.21",
    "postcss-modules": "^6.0.0",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.55.0",
    "shrink-ray-current": "^4.1.3"
  },
  "optionalDependencies": {
    "stylus": "^0.59.0"
  }
}